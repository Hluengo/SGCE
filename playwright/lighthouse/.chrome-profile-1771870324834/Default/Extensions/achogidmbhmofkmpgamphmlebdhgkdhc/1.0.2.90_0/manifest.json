{
   "action": {
      "default_popup": "index.html"
   },
   "background": {
      "service_worker": "background.js",
      "type": "module"
   },
   "content_scripts": [ {
      "js": [ "content-script.js" ],
      "matches": [ "https://*.sodapdf.com/silent-refresh.html*", "https://*.sodapdf.com/*/silent-refresh.html*" ],
      "run_at": "document_end"
   }, {
      "js": [ "pdf-interceptor.js" ],
      "matches": [ "file:///*.pdf" ],
      "run_at": "document_end"
   }, {
      "js": [ "dom-listener.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "description": "Edit and read any PDF files in Chrome. Soda PDF is the ultimate online PDF editor and viewer for any device!",
   "host_permissions": [ "*://*/*" ],
   "icons": {
      "128": "assets/images/logo_128.png",
      "16": "assets/images/logo_16.png",
      "19": "assets/images/logo_19.png",
      "48": "assets/images/logo_48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "Soda PDF Viewer: Edit, Convert, Compress PDF files",
   "permissions": [ "tabs", "cookies", "storage", "webRequest", "webNavigation", "system.display" ],
   "short_name": "Soda PDF Viewer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2.90",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "favicon.ico", "open-dialog.html" ]
   } ]
}
